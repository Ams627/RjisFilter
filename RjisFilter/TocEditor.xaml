<Window x:Class="RjisFilter.TocEditor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RjisFilter"
        mc:Ignorable="d"
        Title="TocEditor" Height="600" Width="800">
    <Grid>
        <DataGrid AutoGenerateColumns="False" MinHeight="100" MinColumnWidth="100">
            <DataGrid.Columns>
                <DataGridTextColumn Header="NLC" Binding="{Binding Nlc}" />
                <DataGridTextColumn Header="Name" Binding="{Binding Name}" />
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
